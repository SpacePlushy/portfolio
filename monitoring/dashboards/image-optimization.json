{
  "dashboard": {
    "id": null,
    "title": "Image Optimization Performance",
    "tags": ["image-optimization", "performance", "processing"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "15s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Image Processing Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(image_optimizations_total[5m])",
            "legendFormat": "Optimizations/sec"
          },
          {
            "expr": "image_processing_queue_size",
            "legendFormat": "Queue Size"
          },
          {
            "expr": "avg(image_processing_duration_seconds)",
            "legendFormat": "Avg Processing Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "list", "orientation": "horizontal"}
          }
        },
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Processing Time by Format",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(image_processing_duration_seconds_bucket{format=\"jpeg\"}[5m]))",
            "legendFormat": "JPEG P95"
          },
          {
            "expr": "histogram_quantile(0.95, rate(image_processing_duration_seconds_bucket{format=\"png\"}[5m]))",
            "legendFormat": "PNG P95"
          },
          {
            "expr": "histogram_quantile(0.95, rate(image_processing_duration_seconds_bucket{format=\"webp\"}[5m]))",
            "legendFormat": "WebP P95"
          },
          {
            "expr": "histogram_quantile(0.95, rate(image_processing_duration_seconds_bucket{format=\"avif\"}[5m]))",
            "legendFormat": "AVIF P95"
          }
        ],
        "yAxes": [{"label": "Seconds", "min": 0}],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 3,
        "title": "File Size Reduction",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(image_size_reduction_ratio) * 100",
            "legendFormat": "Average Size Reduction %"
          },
          {
            "expr": "avg(image_size_before_bytes) / 1024",
            "legendFormat": "Avg Input Size (KB)"
          },
          {
            "expr": "avg(image_size_after_bytes) / 1024",
            "legendFormat": "Avg Output Size (KB)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 4,
        "title": "Processing Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(image_optimizations_success_total[5m]) / rate(image_optimizations_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 14}
      },
      {
        "id": 5,
        "title": "Worker Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "image_worker_active_jobs",
            "legendFormat": "Active Jobs"
          },
          {
            "expr": "image_worker_cpu_usage",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "image_worker_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 14}
      },
      {
        "id": 6,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(image_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(image_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          },
          {
            "expr": "image_cache_size_bytes / 1024 / 1024",
            "legendFormat": "Cache Size (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 14}
      },
      {
        "id": 7,
        "title": "Error Analysis",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(image_processing_errors_total{error_type=\"timeout\"}[5m])",
            "legendFormat": "Timeout Errors/sec"
          },
          {
            "expr": "rate(image_processing_errors_total{error_type=\"format_unsupported\"}[5m])",
            "legendFormat": "Format Errors/sec"
          },
          {
            "expr": "rate(image_processing_errors_total{error_type=\"size_exceeded\"}[5m])",
            "legendFormat": "Size Errors/sec"
          },
          {
            "expr": "rate(image_processing_errors_total{error_type=\"memory\"}[5m])",
            "legendFormat": "Memory Errors/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22}
      },
      {
        "id": 8,
        "title": "Quality Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(image_quality_score)",
            "legendFormat": "Average Quality Score"
          },
          {
            "expr": "avg(image_compression_ratio)",
            "legendFormat": "Compression Ratio"
          }
        ],
        "yAxes": [
          {"label": "Score/Ratio", "min": 0, "max": 1}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22}
      },
      {
        "id": 9,
        "title": "Format Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (format) (rate(image_optimizations_total[5m]))",
            "legendFormat": "{{format}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30}
      },
      {
        "id": 10,
        "title": "Size Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(image_size_histogram_bucket[5m])",
            "legendFormat": "Size Distribution"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30}
      }
    ]
  },
  "overwrite": true
}